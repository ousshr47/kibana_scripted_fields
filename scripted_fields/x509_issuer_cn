if (doc['suricata.eve.tls.issuerdn'].size() != 0)
{
    String full = doc['suricata.eve.tls.issuerdn'].value;
    int lastSlashIndex = full.lastIndexOf('=');
    if (lastSlashIndex > 0)
    {
        String a = full.substring(lastSlashIndex, full.length());
        String cn = a.substring(1, a.length());
        return cn;
    }
    return "";
}
